// Prisma schema untuk MongoDB
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model produk {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  nama_barang      String
  stok             Int
  jumlah_terjual   Int
  tanggal_transaksi DateTime   @default(now()) @map("tanggal_transaksi")
  jenis_barang     String
}